{
    "name": "joinmeet",
    "description": "An awesome extension to autojoin Google Meet!",
    "manifest_version": 3,
    "version": "1.0",
    "permissions": ["activeTab", "scripting", "tabs", "alarms"],
    "host_permissions": ["*://*/*"],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "32": "/images/logo32.png",
        "128": "/images/logo128.png"
    }
}